
@{
    this.ViewData["Title"] = "Landing...";
    Layout = "~/Views/Shared/_LandingLayoutcshtml.cshtml";
}
@using BMS.Models;
@model UserInputModel
<div id="sky">
    <div id="cloud1" class="cloud"></div>
    <div id="cloud2" class="cloud"></div>
    <div id="cloud3" class="cloud"></div>
    <div id="cloud4" class="cloud"></div>
    <div id="cloud5" class="cloud"></div>
    <div id="cloud6" class="cloud"></div>
    <div id="cloud7" class="cloud"></div>
    <div id="aeroplane"></div>
</div>
<div class="container-fluid">
   <div class="row">
       <div class="col">
           <button id="loginButton"type="button" class="btn btn-primary btn-lg" onclick="showHideRegisterForm()">Show register</button>
           <div id="loginForm"class="form-group" style="display:none">
               <label>Register</label>
               <input type="text" class="form-control" placeholder="Desi koga shte piem kafe?"/>
           </div>
       </div>
       <div class="col">
           <button id="registerButton" type="button" class="btn btn-primary btn-lg" onclick="showHideLoginForm()">Show login</button>
           <div id="registerForm" class="form-group" style="display:none">
               <label>Login</label>
               <input type="text" class="form-control" placeholder="Maika ti 2" />
           </div>
       </div>
   </div>
</div>



